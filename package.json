{
  "name": "sonar-scanner-gui",
  "version": "1.0.0",
  "description": "A minimalist material design GUI wrapped over the SonarQube CLI Scanner",
  "main": "resources/js/main.js",
  "scripts": {
    "start": "electron .",
    "build-win32": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=resources/images/icon.ico --prune=true --out=dist --version-string.ProductName=\"SonarQube Scanner GUI\"",
    "build-win64": "electron-packager . --overwrite --platform=win32 --arch=x64 --icon=resources/images/icon.ico --prune=true --out=dist --version-string.ProductName=\"SonarQube Scanner GUI\""
  },
  "repository": "https://github.com/rwitam-b/sonar-scanner-gui",
  "keywords": [
    "sonar-scanner-gui",
    "sonarqube-cli-scanner",
    "node-js",
    "electron-app",
    "electron",
    "material-design"
  ],
  "author": "Rwitam Bandyopadhyay",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^14.0.6"
  },
  "dependencies": {
    "jquery": "^3.5.0",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "roboto-fontface": "^0.10.0",
    "path": "^0.12.7",
    "properties-reader": "0.3.1",
    "sonar-scanner": "^3.1.0"
  }
}
